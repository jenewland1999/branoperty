<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="branoperty.controller.branch.ViewBranch">
   <children>
      <Text layoutX="116.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="View Branch" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Label fx:id="lblUsername" layoutX="66.0" layoutY="112.0" text="Username / Branch Name" />
      <TextField fx:id="tfUsername" editable="false" layoutX="66.0" layoutY="129.0" prefHeight="32.0" prefWidth="288.0" promptText="e.g. northampton">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label fx:id="lblPassword" layoutX="66.0" layoutY="171.0" text="Password" />
      <PasswordField fx:id="pfPassword" editable="false" layoutX="66.0" layoutY="188.0" prefHeight="32.0" prefWidth="288.0" promptText="e.g. password123">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblAddress" layoutX="66.0" layoutY="232.0" text="Address" />
      <TextArea fx:id="taAddress" editable="false" layoutX="66.0" layoutY="249.0" prefHeight="96.0" prefWidth="288.0" promptText="e.g. 10 Gold Road, Northampton, Northamptonshire, GB." wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </TextArea>
      <Label fx:id="lblWebsite" layoutX="66.0" layoutY="356.0" text="Website" />
      <TextField fx:id="tfWebsite" editable="false" layoutX="66.0" layoutY="373.0" prefHeight="32.0" prefWidth="288.0" promptText="e.g. domain.tld/branches/northampton">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label fx:id="lblEmail" layoutX="66.0" layoutY="412.0" text="Email" />
      <TextField fx:id="tfEmail" editable="false" layoutX="66.0" layoutY="429.0" prefHeight="32.0" prefWidth="288.0" promptText="e.g. northampton@domain.tld">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label fx:id="lblTelNo" layoutX="66.0" layoutY="465.0" text="Telephone No." />
      <TextField fx:id="tfTelNo" editable="false" layoutX="66.0" layoutY="482.0" prefHeight="32.0" prefWidth="288.0" promptText="e.g. 7700900461">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <ButtonBar layoutY="552.0" prefHeight="48.0" prefWidth="400.0">
         <buttons>
            <Button fx:id="btnClose" cancelButton="true" defaultButton="true" mnemonicParsing="false" onAction="#close" text="Close" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </buttons>
         <padding>
            <Insets right="8.0" />
         </padding>
      </ButtonBar>
   </children>
</AnchorPane>
