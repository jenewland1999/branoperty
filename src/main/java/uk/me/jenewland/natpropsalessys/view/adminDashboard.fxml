<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.me.jenewland.natpropsalessys.controller.AdminDashboardController">
   <children>
      <TabPane fx:id="tabPaneDashboard" prefHeight="768.0" prefWidth="1024.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="tableBranch" text="Branches">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="tableViewBranch" layoutX="14.0" layoutY="194.0" prefHeight="348.0" prefWidth="997.0">
                          <columns>
                            <TableColumn fx:id="tabColBranchName" prefWidth="136.0" text="Name/Username" />
                            <TableColumn fx:id="tabColBranchAddress" prefWidth="113.0" text="Address" />
                              <TableColumn fx:id="tabColBranchEmail" minWidth="0.0" prefWidth="113.0" text="Email" />
                              <TableColumn fx:id="tabColBranchWeb" minWidth="7.0" prefWidth="121.0" text="Website" />
                              <TableColumn fx:id="tabColBranchTel" prefWidth="97.0" text="Telephone No." />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <Text fx:id="txtWelcomeMsg1" layoutX="14.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, User ">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Text>
                        <Button fx:id="btnLogout1" layoutX="932.0" layoutY="7.0" mnemonicParsing="false" onAction="#logout" text="Logout" textAlignment="CENTER">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <TextField fx:id="tfBranchSearch" layoutX="786.0" layoutY="158.0" onAction="#searchBranches" onKeyTyped="#searchBranches" />
                        <Button fx:id="btnBranchSearch" layoutX="953.0" layoutY="158.0" mnemonicParsing="false" onAction="#searchBranches" text="Search" />
                        <Text layoutX="14.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Branches" />
                        <Button fx:id="btnBranchEdit" layoutX="826.0" layoutY="551.0" mnemonicParsing="false" onAction="#updateBranch" text="Edit Branch" />
                        <Button fx:id="btnBranchDelete" layoutX="915.0" layoutY="551.0" mnemonicParsing="false" onAction="#deleteBranch" text="Delete Branch" />
                        <Button fx:id="btnBranchCreate" layoutX="14.0" layoutY="551.0" mnemonicParsing="false" onAction="#createBranch" text="Create Branch" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="tabProperties" text="Properties">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Text fx:id="txtWelcomeMsg2" layoutX="14.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, User ">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Text>
                        <Button fx:id="btnLogout2" layoutX="932.0" layoutY="7.0" mnemonicParsing="false" onAction="#logout" text="Logout" textAlignment="CENTER">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <TableView fx:id="tableViewProperty" layoutX="14.0" layoutY="194.0" prefHeight="348.0" prefWidth="997.0">
                           <columns>
                              <TableColumn fx:id="tabColPropertyAddress" prefWidth="136.0" text="Address" />
                              <TableColumn fx:id="tabColPropertyRooms" prefWidth="113.0" text="Rooms" />
                              <TableColumn fx:id="tabColPropertySellPrice" minWidth="0.0" prefWidth="113.0" text="Selling Price" />
                              <TableColumn fx:id="tabColPropertySoldPrice" minWidth="7.0" prefWidth="121.0" text="Sold Price" />
                              <TableColumn fx:id="tabColPropertyFloors" prefWidth="147.0" text="Floors" />
                              <TableColumn fx:id="tabColPropertyGarage" prefWidth="144.0" text="Garage (Yes/No)" />
                              <TableColumn fx:id="tabColPropertyGarden" prefWidth="108.0" text="Garden (Yes/No)" />
                              <TableColumn fx:id="tabColPropertyFloor" prefWidth="147.0" text="Floor" />
                              <TableColumn fx:id="tabColPropertyCharge" prefWidth="97.0" text="Monthly Charge" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <TextField fx:id="tfPropertySearch" layoutX="786.0" layoutY="158.0" onAction="#searchProperties" />
                        <Button fx:id="btnPropertySearch" layoutX="953.0" layoutY="158.0" mnemonicParsing="false" onAction="#searchProperties" text="Search" />
                        <Text layoutX="14.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Properties" />
                        <Button fx:id="btnPropertyEdit" layoutX="810.0" layoutY="551.0" mnemonicParsing="false" onAction="#updateProperty" text="Edit Property" />
                        <Button layoutX="907.0" layoutY="551.0" mnemonicParsing="false" onAction="#deleteProperty" text="Delete Property" />
                        <Button fx:id="btnPropertyCreate" layoutX="14.0" layoutY="551.0" mnemonicParsing="false" onAction="#createProperty" text="Create Property" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
